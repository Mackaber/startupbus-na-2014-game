<%= simple_form_for @conductor do |f| %>
  <%= f.input :name %>
  <%= f.input :email %>
  <%= f.input :phone_number %>
  <%= f.input :description %>
  <%= f.input :blog %>
  <%= f.input :facebook %>
  <%= f.input :foursquare %>
  <%= f.input :github %>
  <%= f.input :linkedin %>
  <%= f.input :twitter %>
  <%= f.input :website %>

  <%= f.submit %>
<% end %>
